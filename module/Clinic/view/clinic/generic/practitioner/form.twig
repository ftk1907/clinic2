{% set title = 'Add Practitioner' %}
{% do headTitle(title) %}

<div class="page-header">
    <h1>
        {{ title }}
    </h1>
</div>
{% do form.prepare() %}

{{ form().openTag(form)|raw }}

{% for element in form.getElements() %}
    <div class='form-group'>
        {{ formRow(element) }}
    </div>
{% endfor %}
{{ form().closeTag()|raw }}