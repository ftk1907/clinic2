{% set title = 'Patient Complaints' %}
{% do headTitle(title) %}

<div class="page-header">
    <h1>
        {{ title }}
    </h1>
</div>
{% do form.prepare() %}

{{ form().openTag(form)|raw }}
    <div class='form-group'>
        {{ formRow(form.get('complaints')) }}
    </div>
    <div class='form-group'>
        {{ formRow(form.get('submit')) }}
    </div>
{{ form().closeTag()|raw }}